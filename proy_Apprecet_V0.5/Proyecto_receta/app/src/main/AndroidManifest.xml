<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ramiromadriaga.viewpagerlistviewactivity" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ccahuana.appreceta.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>

        <activity android:name="com.ccahuana.appreceta.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.ccahuana.appreceta.Listarlugaresimg"
            android:parentActivityName="com.ccahuana.appreceta.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ccahuana.appreceta.MainActivity" />
        </activity>
        <activity android:name="com.ccahuana.appreceta.ListarLugares"
            android:parentActivityName="com.ccahuana.appreceta.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ccahuana.appreceta.MainActivity" />
        </activity>
    </application>

</manifest>
